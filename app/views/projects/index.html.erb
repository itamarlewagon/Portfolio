<div class="background-container">
  <img id="moon" src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/1231630/moon2.png" alt="">
  <div class="page-wrapper">
    <h2>DO WHAT YOU LOVE</h2>
  </div>
  <div class="stars"></div>
  <div class="twinkling"></div>
  <div class="clouds"></div>
</div>

<div class="cards">
  <% @projects.each do |project| %>
    <a href="<%= course.url %>"><div class="card" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('https://images.unsplash.com/photo-1554290712-e640351074bd?ixlib=rb-1.2.1&auto=format&fit=crop&w=665&q=80')">
      <h4><%= course.name %></h4>
      <p><%= course.description %></p>
    </div></a>
  <% end %>    
</div>